#include "stm32l4xx_hal.h"
#include"adc.h"

ADC_HandleTypeDef hadc1;
DMA_HandleTypeDef hdma_adc1;

uint32_t sensor_value[1];

int main()
{
	HAL_Init();
	ADC_HandleTypeDef hadc1;
	HAL_ADC_Start_DMA(&hadc1,(uint32_t *)sensor_value,1);
	while(1)
	{

		//HAL_Delay(10);
	}
}


void SysTick_Handler(void)
{
	HAL_IncTick();
}

